{
	"name": "prisma-client-js",
	"type": "module",
	"main": "./src/index.mjs",
	"types": "./src/index.d.mts",
	"scripts": {
		"generate": "prisma generate",
		"migrate": "prisma migrate dev",
		"postinstall": "run-s generate",
		"reset": "prisma migrate reset",
		"studio": "prisma studio"
	},
	"tsup": {
		"format": [
			"esm"
		],
		"dts": {
			"resolve": true,
			"entry": [
				"./index.ts"
			],
			"compilerOptions": {
				"moduleResolution": "node"
			}
		},
		"sourcemap": true,
		"clean": true,
		"target": "es6"
	}
}
