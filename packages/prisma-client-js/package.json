{
	"name": "prisma-client-js",
	"main": "./index.ts",
	"scripts": {
		"postinstall": "npm-run-all prisma:generate:*",
		"prisma:generate:basic": "prisma generate --schema=./prisma/basic/schema.prisma",
		"prisma:migrate:basic": "prisma migrate dev --schema=./prisma/basic/schema.prisma -n init",
		"prisma:migrate-all": "npm-run-all prisma:migrate:*"
	},
	"dependencies": {
		"@prisma/client": "5.20.0",
		"prisma": "^5.20.0"
	}
}
